    "ethers": "^5.0.26",
    "jshashes": "^1.0.8",
    "qrcode": "^1.4.4",
    "semver": "^7.3.4",
    "vue": "^2.6.12",
    "vue-moment": "^4.1.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-router": "^4.5.11",
